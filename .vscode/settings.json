{
  "css.validate": false,
  "emmet.includeLanguages": {
    "tailwindcss": "postcss"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "stylelint.snippet": [
    "tailwindcss"
  ],
  "stylelint.validate": [
    "tailwindcss",
    "vue"
  ],
  "prettier.printWidth": 200,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "phpsab.snifferMode": "onType",
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.fixAll.eslint": true,
    "source.organizeImports": false
  },
  "eslint.codeAction.showDocumentation": {
    "enable": false
  },
  "eslint.quiet": true,
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "phpsab.fixerEnable": true,
  "phpsab.snifferEnable": true,
  "phpsab.executablePathCS": "./vendor/bin/phpcs",
  "phpsab.executablePathCBF": "./vendor/bin/phpcbf",
  "phpsab.standard": "./node_modules/@ueberbit/phpcs-config-drupal/ruleset.xml",
  "phpsab.debug": true,
  "eslint.alwaysShowStatus": false,
  "eslint.format.enable": true,
  "eslint.packageManager": "pnpm",
  "eslint.run": "onSave",
  "eslint.runtime": "/usr/local/opt/node@18/bin/node"
}
